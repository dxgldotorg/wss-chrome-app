{
	"name": "WOT Safe Search",
	"description": "WOT Safe Search protects you from bad sites",
	"version": "1",
	"manifest_version": 2,
	"app": {
		"urls": [
			"*://search.mywot.com/"
		],
		"launch": {
			"web_url": "https://search.mywot.com/?utm_source=cr-wss"
		}
	},
	"icons": {
		"128": "icon128.png"
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
	]
}
